{
  "name": "monster",
  "version": "1.0.0",
  "description": "monster",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/client/index.js ; node ./src/server/index.js ",
    "client": "webpack-dev-server --mode=development --devtool inline-source-map --hot",
    "buildF": "webpack -p",
    "buildB": "webpack --config  webpack_backend.config.js -p",
    "server": "nodemon --exec babel-node  --config-file=./bablelrc_backend src/server/index.js",
    "serverProd": "node --exec babel-node  src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
